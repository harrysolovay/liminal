{
  "[dockercompose]": {
    "editor.defaultFormatter": "dprint.dprint"
  },
  "[html]": {
    "editor.defaultFormatter": "dprint.dprint"
  },
  "[json]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[markdown]": {
    "editor.defaultFormatter": "dprint.dprint"
  },
  "[sql]": {
    "editor.defaultFormatter": "dprint.dprint"
  },
  "[typescript]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[yaml]": {
    "editor.defaultFormatter": "dprint.dprint"
  },
  "biome.enabled": true,
  "editor.formatOnSave": true,
  "editor.quickSuggestions": {
    "strings": "on"
  },
  "editor.tabSize": 2,
  "sqltools.connections": [
    {
      "server": "localhost",
      "port": 54321,
      "driver": "PostgreSQL",
      "name": "liminal",
      "database": "db",
      "username": "username",
      "password": "password"
    }
  ],
  "typescript.preferences.autoImportSpecifierExcludeRegexes": ["^(node:)?os$"]
}
